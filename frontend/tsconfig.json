{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "outDir": "./build",
    "ignoreDeprecations": "6.0"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
